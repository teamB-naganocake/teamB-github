<h1>商品一覧</h1>
<div>
    <% items.each do |item| %>
    <table>
        <tr>
            <th>
                <%= link_to item_path(item) do %>
                  <%= attachment_image_tag(item, :imgge,size: "100x100") %>
                <% end %>
            </th>
        </tr>
        <tr>
          <th>
            <%= link_to item.name, item_path(item) =%>
          </th>
        </tr>
        <tr>
            <th><%= item.price %>円</th>
        </tr>
    </table>
</div>
<% end %>